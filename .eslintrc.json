{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "rules": {
        "@typescript-eslint/consistent-type-imports": 0,
        "@typescript-eslint/space-before-function-paren": 0,
        "@typescript-eslint/strict-boolean-expressions": 0
      }
}
